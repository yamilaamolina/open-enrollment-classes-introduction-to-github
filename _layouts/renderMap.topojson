---

---
{% if page.showJS %}
eqfeed_callback(
{% endif %}

  {
    "type": "FeatureCollection",
    "features": [

    {% for pin in site.pins %}

    {% if pin.longitude %}
    {% capture longitude %}{{ pin.longitude }}{% endcapture %}
    {% elsif pin.Longitude %}
    {% capture longitude %}{{ pin.Longitude }}{% endcapture %}
    {% endif %}
    {% if pin.latitude %}
    {% capture latitude %}{{ pin.latitude }}{% endcapture %}
    {% elsif pin.Latitude %}
    {% capture Latitude %}{{ pin.Latitude }}{% endcapture %}
    {% endif %}

    {% assign chars = longitude | split: "" %}
    {% assign long_is_number = true %}
    {% assign decimals = 0 %}
    {% assign i = 0 %}

    {% for char in chars %}
      {% if i == 0 %}
        {% unless "-0123456789" contains char %}
          {% assign long_is_number = false %}
        {% endunless %}
      {% elsif "0123456789" contains char %}
      {% elsif char == "." and decimals == 0 %}
        {% assign decimals = 1 %}
      {% else %}
        {% assign long_is_number = false %}
      {% endif %}
      {% assign i = i | plus: 1 %}
    {% endfor %}

    {% assign chars = latitude | split: "" %}
    {% assign lat_is_number = true %}
    {% assign decimals = 0 %}
    {% assign i = 0 %}

    {% for char in chars %}
      {% if i == 0 %}
        {% unless "-0123456789" contains char %}
          {% assign lat_is_number = false %}
        {% endunless %}
      {% elsif "0123456789" contains char %}
      {% elsif char == "." and decimals == 0 %}
        {% assign decimals = 1 %}
      {% else %}
        {% assign lat_is_number = false %}
      {% endif %}
      {% assign i = i | plus: 1 %}
    {% endfor %}

      {
        "type": "Feature",
        "geometry": {
          "type": "Point",
          "coordinates": [
            {% if long_is_number %}
            {{ longitude }},
            {% endif %}
            {% if lat_is_number %}
            {{ latitude }},
            {% endif %}
            ]
          },
          "properties": {
            {% if pin.githubHandle %}
            "username": "{{ pin.githubHandle }}"
            {% elsif pin.githubhandle %}
            "username": "{{ pin.githubhandle }}"
            {% endif %}
          }
        }{% if forloop.last == false %},{% endif %}

    {% endfor %}
  ]
  }

{% if page.showJS %}
)
{% endif %}
